<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_eliterp_pedido_venta_id">
            <field name="name">Numero Pedido de Venta</field>
            <field name="code">sale.order.pedido.venta</field>
            <field name="prefix">PDV-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
        </record>

        <record model="ir.sequence" id="seq_eliterp_cotizacion_id">
            <field name="name">Numero Cotizacion de Venta</field>
            <field name="code">sale.order.cotizacion</field>
            <field name="prefix">CDV-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>